    .section .text
    .globl _start

_start:
    li t0, 0x00002000      # GPIO MMIO address
    li t1, 0               # counter

loop:
    sw t1, 0(t0)           # write counter to GPIO
    addi t1, t1, 1         # increment
    andi t1, t1, 255       # keep in 0â€“255

    li a0, 50000
delay:
    addi a0, a0, -1
    bnez a0, delay

    j loop
